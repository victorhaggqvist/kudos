{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "quotes": [1, "single", "avoid-escape"],
        "curly": [1, "multi-line"],

        "no-unused-vars": [0, {"vars": "local", "args": "after-used"}],
        "no-console": [1]
    },
    "extends": "eslint:recommended",
    "global": {
        "Kudoable": true
    }
}
